{
    "name": "kaihei777/laravel-aws-cache-adapter",
    "description": "Laravel Cache Adapter for AWS Credential Caching. Allows you to reduce calls to the ec2 metadata api.",
    "keywords": [
        "laravel",
        "aws",
        "iam",
        "cache",
        "credential"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LukeWaite\\LaravelAwsCacheAdapter\\": "src/"
        }
    },
    "require": {
        "illuminate/support": "^8.0",
        "composer/composer": "^2.1",
        "mockery/mockery": "^1.5",
        "aws/aws-sdk-php": "^3.222",
        "symfony/mailer": "^6.0"
    },
    "require-dev": {
        "laravel/framework": "^8.12",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "scrutinizer/ocular": "^1.9"
    },
    "scripts": {
        "cs": "phpcs --standard=psr2 src/",
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
